{
    "extends": "tslint:recommended",
    "rules": {
        "quotemark": [
            true,
            "single"
        ],
        "no-console": false,
        "arrow-parens": false,
        "interface-name": false,
        "no-angle-bracket-type-assertion": false,
        "object-literal-sort-keys": false,
        "curly": [true, "ignore-same-line"],
        "variable-name": [
            true,
            "ban-keywords",
            "check-format",
            "allow-leading-underscore"
        ]
    }
}
